<?xml version="1.0" encoding="UTF-8"?><widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="io.powers.Pour" version="0.1">
    <name>Pour</name>

    <description>make exceptional coffee</description>

    <author>Bobby Powers &lt;bobbypowers@gmail.com></author>

    <preference name="KeyboardShrinksView" value="true"/>
    <preference name="StatusBarOverlaysWebView" value="false"/>
    <preference name="StatusBarBackgroundColor" value="#000000"/>
    <preference name="iosPersistentFileLocation" value="Library"/>
    <preference name="AndroidPersistentFileLocation" value="Internal"/>

    <content src="plugins/org.chromium.bootstrap/chromeapp.html"/>
    <preference name="android-minSdkVersion" value="14"/>
<access origin="https://pour-app.s3.amazonaws.com/*"/></widget>